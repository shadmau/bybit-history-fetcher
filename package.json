{
  "name": "bybit-history-fetcher",
  "version": "1.0.0",
  "description": "A data fetcher for Bybit, fetching historical market data (klines) and saving it to a JSON file.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.4.5",
    "https-proxy-agent": "^7.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.50.0",
    "husky": "^8.0.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "Bybit",
    "API",
    "Data Fetcher",
    "TypeScript",
    "Klines",
    "Cryptocurrency"
